local SeedPackController = require(game.ReplicatedStorage.Modules.SeedPackController)
local SeedPackData = require(game.ReplicatedStorage.Data.SeedPackData)

local packName = "Flower Seed Pack"
local seedName = "Sunflower"

local items = SeedPackData.Packs[packName].Items
local index = 1

for i, item in ipairs(items) do
	if item.Name == seedName or item.RewardId == seedName then
		index = i
		break
	end
end

SeedPackController:Spin({
	seedPackType = packName,
	resultIndex = index
})
